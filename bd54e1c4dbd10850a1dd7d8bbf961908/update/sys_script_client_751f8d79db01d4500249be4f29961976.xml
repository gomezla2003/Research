        <script><![CDATA[function onLoad() {
   //Type appropriate comment here, and begin script below
	var arm = g_form.getValue('arm');
	
	var ga = new GlideAjax('ResearchUtil');
ga.addParam('sysparm_name', 'getStudyName'); 
ga.addParam('sysparm_arm', arm); 
ga.getXML(callme);

function callme(response) {
		var answer = response.responseXML.documentElement.getAttribute("answer");
			answer = answer.evalJSON();
	g_form.setValue('short_description',answer.studyName);
	g_form.setValue('u_arm_name',answer.armName);
	
}
	
	
	
}]]></script>
