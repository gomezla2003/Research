        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    var key = current.assignment_key.toString();
    var id = current.linked_version;
    var gr = new GlideRecord('x_sthc_research_version');
    gr.get(id);
    switch (key) {
        case 'Medication Nursing Review':
            gr.setValue('medications_revisions_nursing', true);
            break;
        case 'Medication Build Validation':
            gr.setValue('medications_revisions', true);
            break;
        case 'Non-Med Build Validation':
            gr.setValue('non_med_revisions', true);
            break;
        case 'Non-Med Build Nursing Review':
            gr.setValue('non_med_revisions_nursing', true);
            break;
        default:
            gs.log(current.number + 'Error in lookup for Business Rule Validation Revisions');

    }

    gr.update();

})(current, previous);]]></script>
